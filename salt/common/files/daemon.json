{%- set DOCKERRANGE = salt['pillar.get']('docker:range') %}
{%- set DOCKERBIND = salt['pillar.get']('docker:bip') %}
{
    "registry-mirrors": [ "https://:5000" ],
    "bip": "{{ DOCKERBIND }}",
    "default-address-pools": [
      {
        "base" : "{{ DOCKERRANGE }}",
        "size" : 24
      }
    ]
}